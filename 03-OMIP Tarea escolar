class program {
define turn(n){
	iterate(n){
    	turnleft();
    }
}
    program () {
      while(nextToABeeper){
      	pickbeeper();
      }
      turn(3);
      move();
      while(nextToABeeper){
      	pickbeeper();
      }
      while(anyBeepersInBeeperBag){
      	putbeeper();
        turn(2);
      }
        turnoff();
    }
}
